<?xml version="1.0" encoding="UTF-8"?>
<setup:Product
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:com.github.maybeec.oomph.task="http://maybeec.github.io/oomph-task-unzip/1.0"
    xmlns:com.github.maybeec.oomph.task_1="http://maybeec.github.io/oomph-task-cli/1.0"
    xmlns:fscopy="http://com.github.maybeec/task-fscopy/1.0"
    xmlns:fsrename="http://maybeec.github.io/task-fsrename/1.0"
    xmlns:jdt="http://www.eclipse.org/oomph/setup/jdt/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.p2="http://www.eclipse.org/oomph/setup/p2/1.0"
    xsi:schemaLocation="http://maybeec.github.io/oomph-task-unzip/1.0 https://raw.githubusercontent.com/maybeec/oomph-task-unzip/master/task-unzip-updatesite/model/Oomph-task-unzip.ecore http://maybeec.github.io/oomph-task-cli/1.0 https://raw.githubusercontent.com/maybeec/oomph-task-cli/master/task-cli-updatesite/model/Oomph-cli-task-1.0.ecore http://com.github.maybeec/task-fscopy/1.0 https://raw.githubusercontent.com/maybeec/oomph-task-fscopy/master/task-fscopy-updatesite/model/Oomph-task-fscopy.ecore http://maybeec.github.io/task-fsrename/1.0 https://raw.githubusercontent.com/maybeec/oomph-task-fsrename/master/task-fsrename-updatesite/model/Oomph-task-fsrename.ecore http://www.eclipse.org/oomph/setup/jdt/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/JDT.ecore"
    name="oasp4j"
    label="OASP4J">
  <annotation
      source="http://www.eclipse.org/oomph/setup/BrandingInfo">
    <detail
        key="folderName">
      <value>eclipse</value>
    </detail>
    <detail
        key="folderName.macosx">
      <value>Eclipse</value>
    </detail>
    <detail
        key="imageURI">
      <value>https://raw.githubusercontent.com/oasp/oasp4j-ide/master/oasp4j-ide-oomph-setups/resources/oasp-logo.png</value>
    </detail>
    <detail
        key="siteURI">
      <value>https://oasp.github.io/</value>
    </detail>
  </annotation>
  <setupTask
      xsi:type="setup:InstallationTask"
      id="installation"/>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="config.user.input"
      name="unset config as simple user interface">
    <setupTask
        xsi:type="setup:VariableTask"
        id="installation.root"
        type="FOLDER"
        name="installation.root"
        storageURI="scope://Installation"
        label="OASP installation folder">
      <description>Installation Folder</description>
    </setupTask>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="config.versions.noneclipse.resources"
      name="version config of non-eclipse resources">
    <setupTask
        xsi:type="setup:VariableTask"
        id="oasp.scripts.version"
        name="oasp.scripts.version"
        value="1.4.0"
        defaultValue="1.4.0"
        storageURI="scope://Installation"
        label="oasp4j-scripts version">
      <description>OASP Scripts version to be used</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="ant.version"
        name="ant.version"
        value="1.9.7"
        defaultValue="1.9.7"
        storageURI="scope://Installation"
        label="ant version">
      <description>ANT version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="maven.version"
        name="maven.version"
        value="3.5.2"
        defaultValue="3.5.2"
        storageURI="scope://Installation"
        label="maven version">
      <description>Maven version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="tomcat.version"
        name="tomcat.version"
        value="7.0.8"
        defaultValue="7.0.8"
        storageURI="scope://Installation"
        label="tomcat version">
      <description>Tomcat version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="python.version"
        name="python.version"
        value="2.7.8"
        defaultValue="2.7.8"
        storageURI="scope://Installation"
        label="python version">
      <description>Python Version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="nodejs.version"
        name="nodejs.version"
        value="6.9.1"
        defaultValue="6.9.1"
        storageURI="scope://Installation"
        label="nodejs version">
      <description>NodeJS version to be installed</description>
    </setupTask>
    <description>Configuration of product versions of non-eclipse resources</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="config.common.paths"
      name="config of commonly used paths">
    <setupTask
        xsi:type="setup:VariableTask"
        id="software.location"
        name="software.location"
        value="${installation.root/software}"
        storageURI="scope://Installation">
      <description>Software bundles folder</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        name="workspaces.location"
        value="${installation.root/workspaces}"
        storageURI="scope://Installation"
        label="">
      <description></description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        name="settings.location"
        value="${workspaces.location/main}"
        storageURI="scope://Installation"/>
    <setupTask
        xsi:type="setup:VariableTask"
        name="tmp.download.location"
        value="${installation.root/temp}"
        storageURI="scope://Installation">
      <description>Specifies where temporary data will be stored</description>
    </setupTask>
    <description>Configuration of commonly used paths</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="config.reserved.paths"
      name="config of paths overriding reserved oomph variables">
    <setupTask
        xsi:type="setup:VariableTask"
        id="installation.location"
        excludedTriggers="STARTUP MANUAL"
        name="installation.location"
        value="${software.location}"
        storageURI="scope://Installation"/>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="compound.downloads"
      name="download tasks of external resources">
    <setupTask
        xsi:type="setup:ResourceCopyTask"
        id="rc.oasp"
        excludedTriggers="STARTUP MANUAL"
        sourceURL="https://github.com/oasp/oasp4j-ide/archive/master.zip"
        targetURL="${tmp.download.location/oasp4j.zip|uri}">
      <description>Fetches the OASP4J file archive</description>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.download.external.ressources.windows"
        filter="(os.name=Windows*)"
        name="windows">
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.win.ant"
          excludedTriggers="STARTUP MANUAL"
          sourceURL="http://archive.apache.org/dist/ant/binaries/apache-ant-${ant.version}-bin.zip"
          targetURL="${tmp.download.location/ant.zip|uri}">
        <description>Fetches the ant ${ant.version} archive for windows</description>
      </setupTask>
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.win.maven"
          excludedTriggers="STARTUP MANUAL"
          sourceURL="http://www-us.apache.org/dist/maven/maven-3/${maven.version}/binaries/apache-maven-${maven.version}-bin.zip"
          targetURL="${tmp.download.location/maven.zip|uri}">
        <description>Fetches the maven ${maven.version} archive</description>
      </setupTask>
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.win.oasp.scripts"
          excludedTriggers="STARTUP MANUAL"
          sourceURL="http://repo1.maven.org/maven2/io/oasp/java/ide/oasp4j-ide-scripts/${oasp.scripts.version}/oasp4j-ide-scripts-${oasp.scripts.version}-windows.zip"
          targetURL="${tmp.download.location/oasp4jScripts.zip|uri}">
        <description>Fetches the OASP4J scripts archive</description>
      </setupTask>
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.win.help.bat"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.win.oasp.scripts rc.oasp"
          sourceURL="https://raw.githubusercontent.com/oasp/oasp4j-ide/master/oasp4j-ide-oomph-setups/resources/help.bat"
          targetURL="${installation.root/help.bat|uri}">
        <description>Fetches the windows helper script for automatic oasp4j script execution at the end of the installation process</description>
      </setupTask>
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.win.msiiinstall.bat"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.win.oasp.scripts rc.oasp"
          sourceURL="https://raw.githubusercontent.com/oasp/oasp4j-ide/dev_oomph/oasp4j-ide-oomph-setups/resources/msiinstall.bat"
          targetURL="${installation.root/msiinstall.bat|uri}">
        <description>Fetches the windows helper script for installation of msi packages</description>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.windows.x64"
          filter="(osgi.arch=x86_64)"
          name="x64">
        <setupTask
            xsi:type="setup:ResourceCopyTask"
            id="rc.win.x64.tomcat"
            excludedTriggers="STARTUP MANUAL"
            sourceURL="http://archive.apache.org/dist/tomcat/tomcat-7/v${tomcat.version}/bin/apache-tomcat-${tomcat.version}-windows-x64.zip"
            targetURL="${tmp.download.location/tomcat.zip|uri}">
          <description>Fetches the win x64 tomcat ${tomcat.version} archive</description>
        </setupTask>
        <setupTask
            xsi:type="setup:ResourceCopyTask"
            id="rc.win.x64.nodejs"
            excludedTriggers="STARTUP MANUAL"
            successor="install.nodejs"
            filter=""
            sourceURL="https://nodejs.org/dist/v${nodejs.version}/node-v${nodejs.version}-x64.msi"
            targetURL="${tmp.download.location/nodejs.msi|uri}">
          <description>Fetches the x64 nodejs ${nodejs.version} msi installer package</description>
        </setupTask>
        <setupTask
            xsi:type="setup:ResourceCopyTask"
            id="rc.win.x64.python"
            excludedTriggers="STARTUP MANUAL"
            successor="install.python"
            sourceURL="https://www.python.org/ftp/python/${python.version}/python-${python.version}.amd64.msi"
            targetURL="${tmp.download.location/py.msi|uri}">
          <description>Fetches the x64 Python ${python.version} windows installer package</description>
        </setupTask>
        <description>Download tasks for external ressources on windows x64</description>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.windows.x32"
          filter="(osgi.arch=x86)"
          name="x32">
        <setupTask
            xsi:type="setup:ResourceCopyTask"
            id="rc.win.x32.tomcat"
            excludedTriggers="STARTUP MANUAL"
            sourceURL="http://archive.apache.org/dist/tomcat/tomcat-7/v${tomcat.version}/bin/apache-tomcat-${tomcat.version}-windows-x32.zip"
            targetURL="${tmp.download.location/tomcat.zip|uri}">
          <description>Fetches the win x32 tomcat ${tomcat.version} archive</description>
        </setupTask>
        <setupTask
            xsi:type="setup:ResourceCopyTask"
            id="rc.win.x32.nodejs"
            excludedTriggers="STARTUP MANUAL"
            successor="install.nodejs"
            filter=""
            sourceURL="https://nodejs.org/dist/v${nodejs.version}/node-v${nodejs.version}-x86.msi"
            targetURL="${tmp.download.location/nodejs.msi|uri}">
          <description>Fetches the x32 nodejs ${nodejs.version} msi installer package</description>
        </setupTask>
        <setupTask
            xsi:type="setup:ResourceCopyTask"
            id="rc.win.x32.python"
            excludedTriggers="STARTUP MANUAL"
            successor="install.python"
            sourceURL="https://www.python.org/ftp/python/${python.version}/python-${python.version}.msi"
            targetURL="${tmp.download.location/py.msi|uri}">
          <description>Fetches the x32 Python ${python.version} windows installer package</description>
        </setupTask>
        <description></description>
      </setupTask>
      <description>Download tasks for external ressources on windows</description>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.download.external.ressources.linux"
        filter="(osgi.os=linux)"
        name="linux">
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.linux.ant"
          excludedTriggers="STARTUP MANUAL"
          sourceURL="http://archive.apache.org/dist/ant/binaries/apache-ant-${ant.version}-bin.tar.gz"
          targetURL="${tmp.download.location/ant.tar.gz|uri}">
        <description>Fetches the linux ant ${ant.version} archive</description>
      </setupTask>
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.linux.maven"
          excludedTriggers="STARTUP MANUAL"
          sourceURL="http://www-us.apache.org/dist/maven/maven-3/${maven.version}/binaries/apache-maven-${maven.version}-bin.tar.gz"
          targetURL="${tmp.download.location/maven.tar.gz|uri}">
        <description>Fetches the maven ${maven.version} archive for linux</description>
      </setupTask>
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.linux.tomcat"
          excludedTriggers="STARTUP MANUAL"
          sourceURL="http://archive.apache.org/dist/tomcat/tomcat-7/v${tomcat.version}/bin/apache-tomcat-${tomcat.version}.tar.gz"
          targetURL="${tmp.download.location/tomcat.tar.gz|uri}">
        <description>Fetches the tomcat ${tomcat.version} linux package</description>
      </setupTask>
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.linux.python"
          excludedTriggers="STARTUP MANUAL"
          sourceURL="https://www.python.org/ftp/python/${python.version}/Python-${python.version}.tgz"
          targetURL="${tmp.download.location/python.tar.gz|uri}">
        <description>Fetches the python ${python.version} linux package</description>
      </setupTask>
      <setupTask
          xsi:type="setup:ResourceCopyTask"
          id="rc.win.oasp.scripts"
          excludedTriggers="STARTUP MANUAL"
          successor="unzip.oasp.scripts"
          sourceURL="https://github.com/oasp/oasp4j-ide/raw/dev_oomph/oasp4j-ide-oomph-setups/resources/oasp4j-ide-scripts-linux.tar.gz"
          targetURL="${tmp.download.location/oasp4jScripts.tar.gz|uri}">
        <description>Fetches the OASP4J scripts archive</description>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.linux.x64"
          filter="(osgi.arch=x86_64)"
          name="x64">
        <setupTask
            xsi:type="setup:ResourceCopyTask"
            id="rc.linux.x64.nodejs"
            excludedTriggers="STARTUP MANUAL"
            sourceURL="https://nodejs.org/dist/v${nodejs.version}/node-v${nodejs.version}-linux-x64.tar.gz"
            targetURL="${tmp.download.location/nodejs.tar.gz|uri}">
          <description>Fetches the x64 nodejs ${nodejs.version} package for linux</description>
        </setupTask>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.linux.x32"
          filter="(osgi.arch=x86)"
          name="x32">
        <setupTask
            xsi:type="setup:ResourceCopyTask"
            id="rc.linux.x32.nodejs"
            excludedTriggers="STARTUP MANUAL"
            sourceURL="https://nodejs.org/dist/v${nodejs.version}/node-v${nodejs.version}-linux-x86.tar.gz"
            targetURL="${tmp.download.location/nodejs.tar.gz|uri}">
          <description>Fetches the x32 nodejs ${nodejs.version} package for linux</description>
        </setupTask>
      </setupTask>
    </setupTask>
    <description>downloads external resources</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="tasks.unzip.external.resources"
      name="unzip tasks of external resources">
    <setupTask
        xsi:type="com.github.maybeec.oomph.task:unzipTask"
        id="unzip.oasp"
        excludedTriggers="STARTUP MANUAL"
        predecessor="rc.oasp"
        zipFile="${tmp.download.location/oasp4j.zip}"
        destinationDir="${tmp.download.location}"
        priority="100">
      <description>Unzips the oasp into the supposed folder</description>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.download.external.ressources.linux"
        filter="(osgi.os=linux)"
        name="linux">
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.linux.python"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.linux.python"
          zipFile="${tmp.download.location/python.tar.gz}"
          destinationDir="${software.location}"
          priority="100">
        <description>unzips the python tar ball</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.linux.nodejs"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.linux.x64.nodejs rc.linux.x32.nodejs"
          zipFile="${tmp.download.location/nodejs.tar.gz}"
          destinationDir="${software.location}"
          priority="100">
        <description>unarchives the nodejs tar ball</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.ant"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.linux.ant"
          zipFile="${tmp.download.location/ant.tar.gz}"
          destinationDir="${software.location}"
          priority="100">
        <description>Unzips the ant binaries</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.maven"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.linux.maven"
          zipFile="${tmp.download.location/maven.tar.gz}"
          destinationDir="${software.location}"
          priority="100">
        <description>Unzips the maven binaries</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.tomcat"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.linux.tomcat"
          zipFile="${tmp.download.location/tomcat.tar.gz}"
          destinationDir="${software.location}"
          priority="100">
        <description>Unzips the tomcat binaries</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.oasp.scripts"
          excludedTriggers="STARTUP MANUAL"
          zipFile="${tmp.download.location/oasp4jScripts.tar.gz}"
          destinationDir="${installation.root}"
          priority="100">
        <description></description>
      </setupTask>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.download.external.ressources.windows"
        filter="(os.name=Windows*)"
        name="windows">
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.ant"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.win.ant"
          zipFile="${tmp.download.location/ant.zip}"
          destinationDir="${software.location}"
          priority="100">
        <description>Unzips the ant binaries</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.maven"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.win.maven"
          zipFile="${tmp.download.location/maven.zip}"
          destinationDir="${software.location}"
          priority="100">
        <description>Unzips the maven binaries</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.tomcat"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.win.x32.tomcat rc.win.x64.tomcat"
          zipFile="${tmp.download.location/tomcat.zip}"
          destinationDir="${software.location}"
          priority="100">
        <description>Unzips the tomcat binaries</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:unzipTask"
          id="unzip.oasp.scripts"
          excludedTriggers="STARTUP MANUAL"
          predecessor="rc.win.oasp.scripts"
          zipFile="${tmp.download.location/oasp4jScripts.zip}"
          destinationDir="${installation.root}"
          priority="100">
        <description></description>
      </setupTask>
      <description>Download tasks for external ressources on windows</description>
    </setupTask>
    <description></description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="tasks.finalize.external.resources"
      name="finalize external resources">
    <setupTask
        xsi:type="fsrename:FSRenameTask"
        id="rename.ant"
        excludedTriggers="MANUAL"
        predecessor="unzip.ant"
        resource="${software.location/}apache-ant-${ant.version}"
        destination="ant"
        priority="100">
      <description>Removes the version from the ant folder</description>
    </setupTask>
    <setupTask
        xsi:type="fsrename:FSRenameTask"
        id="rename.maven"
        excludedTriggers="MANUAL"
        predecessor="unzip.maven"
        filter=""
        resource="${software.location/}apache-maven-${maven.version}"
        destination="maven"
        priority="100"/>
    <setupTask
        xsi:type="fsrename:FSRenameTask"
        id="rename.tomcat"
        excludedTriggers="MANUAL"
        predecessor="unzip.tomcat"
        resource="${software.location/}apache-tomcat-${tomcat.version}"
        destination="tomcat"
        priority="100"/>
    <setupTask
        xsi:type="fscopy:FileSystemCopyTask"
        id="fscopy.oasp.scripts"
        excludedTriggers="MANUAL"
        predecessor="unzip.oasp.scripts"
        resource="${tmp.download.location/oasp4j-ide-master/oasp4j-ide-settings/src/main/settings}"
        destination="${settings.location/development/settings}"
        priority="100"/>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.finalize.external.resources.win"
        predecessor="rc.win.msiiinstall.bat"
        filter="(os.name=Windows*)"
        name="Windows">
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_1:cliTask"
          id="install.nodejs"
          excludedTriggers="STARTUP MANUAL"
          directory="${installation.root}"
          command="msiinstall.bat"
          priority="100">
        <argument>nodejs</argument>
        <description>
          Extracts NodeJS

        </description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_1:cliTask"
          id="install.nodejs.cleanup.msi"
          excludedTriggers="STARTUP MANUAL"
          directory="${installation.root}"
          command="del"
          priority="100">
        <argument>&quot;${software.location/nodejs.msi}&quot;</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_1:cliTask"
          id="mkdir.python"
          excludedTriggers="STARTUP MANUAL"
          directory="${installation.root}"
          command="mkdir"
          priority="100">
        <argument>${software.location/python}</argument>
        <description>creates the installation directory for python</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_1:cliTask"
          id="install.python"
          excludedTriggers="STARTUP MANUAL"
          predecessor="mkdir.python"
          filter=""
          directory="${installation.root}"
          command="msiinstall.bat"
          priority="100">
        <argument>py</argument>
        <argument>python</argument>
        <description>Install MSI Python Package</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_1:cliTask"
          id="install.python.cleanup"
          excludedTriggers="STARTUP MANUAL"
          directory="${installation.root}"
